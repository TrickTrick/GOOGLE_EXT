{
  "manifest_version": 2,
  "name": "Alert Ext",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Alert Ext",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
}